create table Construction (id SERIAL PRIMARY KEY, building_n int, CHECK(building_n > 0), floor_n int, UNIQUE(building_n, floor_n)); create table ChairProperty (id SERIAL references Construction(id), chair varchar(15), type varchar(25), UNIQUE(chair, type)); 

